
@{
    ViewBag.Title = "PortfolioUsuario";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="conteudoPortfolio">

    @{int i = 0;}
    @foreach (var item in ViewBag.imagens)
    {
        <div class="card">
            <img src="@item" alt="@ViewBag.nomes[i]" style="width:100%">
            <div>
                <div class="container" style="height:1100px">
                    <h4><b>@ViewBag.nomes[i]</b></h4>
                    <p>@ViewBag.tipos[i]</p>
                </div>
                <a href="@ViewBag.links[i]" class="btn btn-default" role="button" style="margin:0 0 5% 30%; background-color:#ed8239; color:#ffffff ">Veja mais...</a>
            </div>
        </div>

        i++;
    }

</div>
