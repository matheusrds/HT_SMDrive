@{
    ViewBag.Title = "Inventário";
}
<div id="conteudoPortfolio">

    @if (ViewBag.Usuarios != null)
    {
        foreach (var item in ViewBag.Usuarios)
        {
            <div class="card">
                <div id="foto" alt="Inventário"></div>
                <div class="container">
                    <h4 style="color:#06586f"><b>Título</b></h4>
                    <p>@item.UserName</p>
                    <p>Área de interesse</p>
                    <a href="#" class="btn btn-default" role="button" style="margin:15% 0% 5% 30%; background-color:#ed8239; color:#ffffff ">Portfolio</a>
                </div>
            </div>
        }
    }
    else
    {
        <h4> <br/> <br/> <br/> Ops... Ocorreu um erro ao carregar as informações :( <br />  Tente atualizar a página! </h4>
    }


</div>